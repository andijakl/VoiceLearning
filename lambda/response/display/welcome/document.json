{
  "type": "APL",
  "version": "1.7",
  "theme": "dark",
  "import": [
    {
      "name": "alexa-layouts",
      "version": "1.5.0"
    }
  ],
  "background": "#005097",
  "mainTemplate": {
    "parameters": [
      "payload"
    ],
    "items": [
      {
        "type": "AlexaHeadline",
        "primaryText": "${payload.data.primaryText}",
        "secondaryText": "${payload.data.secondaryText}",
        "backgroundColor": "#005097",
        "backgroundOverlayGradient": true,
        "footerHintText": "${payload.data.footerHintText}",
        "headerAttributionImage": "https://alexa-voice-learning-images.s3.eu-west-1.amazonaws.com/VoiceLearningSkill.png",
        "headerBackButton": false,
        "headerDivider": false,
        "headerSubtitle": "${payload.data.headerSubtitle}",
        "headerTitle": "${payload.data.headerTitle}"
      }
    ]
  }
}